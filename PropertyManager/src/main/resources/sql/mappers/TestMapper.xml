<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tmoncorp.PropertyManager.repository.TestMapper">
	<resultMap type="TestModel" id="TestMapperResult">
		<result property="name" column="name" />
		<result property="numbering" column="numbering" />
	</resultMap>
	
	<select id="selectTest" resultMap="TestMapperResult">
		SELECT
			*
		FROM
			test
	</select>
</mapper>